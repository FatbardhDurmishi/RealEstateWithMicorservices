        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Properties List</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="properties-list" class="table table-bordered table-striped table-hover">
