<div class="content">
    <partial name="_Navbar" />
    <div class="container-fluid pt-4 px-4 col-12">
        <!-- Content Header (Page header) -->
        <div class="row">
            @* <div class="col-sm-6 col-xl-3 ">
            @*<h1>Users</h1>
            </div>*@
            <div class="col-6">
                <ol class="breadcrumb float-sm-right col-6 text-start ">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">Properties</li>
                </ol>
            </div>
            <div class="col-6 text-end">
                <a  asp-action="AddProperty" asp-controller="Properties" class="btn btn-primary"><i class="bi bi-plus-circle"></i>&nbsp; Add Property</a>
            </div>
        </div>
    </div>

    <div class="container-fluid pt-4 px-4 col-12">
